b:Button("cancel house",function()
local A_1 = "PlayerSellHouse"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayerHouseChoice
Event:FireServer(A_1)


end)







b:Box("Take house number","number",function(value) -- "number" or "string"

local A_1 = "BuyingHouse"
local A_2 = game:GetService("Workspace")["001_Lots"]["For Sale"].Number.Number
local A_3 = game:GetService("Workspace")["001_Lots"]["For Sale"].BuyHouse
local A_4 = game:GetService("Workspace")["001_Lots"]["For Sale"].BuyHouse.PlotOfLand
local Event = game:GetService("ReplicatedStorage").RemoteEvents.GettingHouse
Event:FireServer(A_1, A_2, A_3, A_4)

    
 
local A_1 = "PickingCustomHouse"
local A_2 = "032_House"
local A_3 = value
local Event = game:GetService("ReplicatedStorage").RemoteEvents.GettingHouse
Event:FireServer(A_1, A_2, A_3)



end)



b:Box("Take house number (PREMIUM)","number",function(value) -- "number" or "string"
 
local A_1 = "BuyingHouse"
local A_2 = game:GetService("Workspace")["001_Lots"]["For Sale"].Number.Number
local A_3 = game:GetService("Workspace")["001_Lots"]["For Sale"].BuyHouse
local A_4 = game:GetService("Workspace")["001_Lots"]["For Sale"].BuyHouse.PlotOfLand
local Event = game:GetService("ReplicatedStorage").RemoteEvents.GettingHouse
Event:FireServer(A_1, A_2, A_3, A_4)

 
local A_1 = "PickingCustomHouse"
local A_2 = "033_House"
local A_3 = value
local Event = game:GetService("ReplicatedStorage").RemoteEvents.GettingHouse
Event:FireServer(A_1, A_2, A_3)



