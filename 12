b:Toggle("Garage",function(bool)
    local A_1 = "GarageDoor"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)
end)

b:Toggle("Curtains",function(bool)
local A_1 = "Curtains"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)
end)


b:Box("Insert house Text","letter",function(value) -- "number" or "string"

 
local A_1 = "BusinessName"
local A_2 = value
local Event = game:GetService("ReplicatedStorage").RemoteEvents.RPHouseEvent
Event:FireServer(A_1, A_2)



end)


b:Toggle("LockDoors",function(bool)
    if bool then
local A_1 = "LockDoors"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)

    else
local A_1 = "LockDoors"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)



    end
end)


b:Box("music (GAMEPASS)","number",function(value) -- "number" or "string"
local A_1 = "P6ickingHouseMusicText"
local A_2 = value
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1, A_2)


end)

b:Toggle("house pool (GAMEPASS)",function(bool)
     if bool then
       local A_1 = "PoolOnOff"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)

    else
        local A_1 = "PoolOnOff"
local Event = game:GetService("ReplicatedStorage").RemoteEvents.PlayersHouse
Event:FireServer(A_1)


    end
end)


